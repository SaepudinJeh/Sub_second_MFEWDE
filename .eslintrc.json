{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "disallowRedundantWrapping": 0,
        "no-undef": 0,
        "prefer-regex-literals": 0
    }
}
